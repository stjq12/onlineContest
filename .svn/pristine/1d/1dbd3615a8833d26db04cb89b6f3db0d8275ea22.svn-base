import request from '@/utils/request'

export function tonobeJudge(tid) {
  return request({
    url: '/api/tonotbeJudge',
    method: 'post',
    params: {
      tid: tid
    }
  })
}

// 检索
export function getJudges(data) {
  return request({
    url: '/api/getCourseJudgeMesByString',
    method: 'post',
    params: {
      realname: data.realname,
      school: data.school,
      major: data.major,
      mid: data.mid,
      pageNum: data.page,
      pageSize: data.limit
    }
  })
}
