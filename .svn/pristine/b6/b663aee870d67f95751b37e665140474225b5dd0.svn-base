import request from '@/utils/request'

// 获得奖项信息
export function getPrizeName() {
  return request({
    url: '/api/getPrizeNameWithID',
    method: 'post',
    params: {

    }
  })
}

export function advance(list) {
  return request({
    url: '/api/toJSWorks',
    method: 'post',
    params: {
      wid: list.wid,
      certificate: list.tempRadio,
      reward_date: list.time
    }
  })
}
